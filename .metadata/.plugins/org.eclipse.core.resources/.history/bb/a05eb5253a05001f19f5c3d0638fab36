package com.kh.thenolja.common.model.vo;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.Getter;
import lombok.Setter;

public class PageInfo {
	private int listCount;
	private int currentPage;
	private int boardLimit;
	private int pageLimit;
	
	private int maxPage;
	private int startPage;
	private int endPage;
	public PageInfo(int listCount2, 
			        int currentPage2,
			        int boardLimit2,
			        int pageLimit2,
			        int maxPage2,
			        int startPage2,
			        int endPage2) 
	{
		this.listCount  = listCount2;
		this.currentPage = currentPage2;
		this.boardLimit = boardLimit2;
		this.pageLimit  = pageLimit2;
		this.maxPage    = maxPage2;
		this.startPage  = startPage2;
		this.endPage    = endPage2;
	}
	public int getCurrentPage() {
		return currentPage;
	}
	public int getBoardLimit() {
		return boardLimit;
	}
}
